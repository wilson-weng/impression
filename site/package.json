{
  "name": "impression-react-site",
  "version": "0.1.14",
  "license": "MIT",
  "description": "An enterprise-class UI design language and React-based implementation.",
  "main": "./lib/index.js",
  "scripts": {
    "eslint": "eslint ./src/scripts/",
    "start": "gulp build && webpack-dashboard -- node server.js",
    "watch": "gulp watch",
    "copy:impression-css": "cp -R ../src/styles/** src/styles/impression",
    "copy:impression-js": "cp -R ../src/scripts/** src/scripts/components/impression",
    "copy": "npm run copy:impression-css && npm run copy:impression-js",
    "cover:impression-css": "cp -R src/styles/impression/** ../src/styles",
    "cover:impression-js": "cp -R src/scripts/components/impression/** ../src/scripts",
    "cover": "npm run cover:impression-css && npm run cover:impression-js",
    "prebuild": "npm run eslint",
    "build": "webpack -p --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shenlq/impression.git"
  },
  "homepage": "https://github.com/shenlq/Impression",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "highlight.js": "^9.6.0",
    "moment": "^2.14.1",
    "perfect-scrollbar": "^0.6.12"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^2.3.2",
    "gulp-watch": "^4.3.9",
    "html-webpack-plugin": "^2.16.0",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.6.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dashboard": "^0.1.1",
    "webpack-dev-server": "^1.14.1"
  }
}
